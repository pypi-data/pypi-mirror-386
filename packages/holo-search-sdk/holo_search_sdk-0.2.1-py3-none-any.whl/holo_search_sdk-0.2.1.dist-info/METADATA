    [1, "æ–‡æ¡£1", "å†…å®¹1", [0.1, 0.2, 0.3]],
    [2, "æ–‡æ¡£2", "å†…å®¹2", [0.4, 0.5, 0.6]],
    [3, "æ–‡æ¡£3", "å†…å®¹3", [0.7, 0.8, 0.9]]
