<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
  <record id="product_coopcycle_service" model="product.product">
    <field name="name">Service (coopcycle)</field>
    <field name="type">service</field>
    <field name="sale_ok">False</field>
    <field name="purchase_ok">False</field>
  </record>
  <record id="ir_config_coopcycle_product" model="ir.config_parameter">
    <field name="key">odoo_coopcycle_connector.coopcycle_product_id</field>
    <field name="value" eval="'product.product,' + str(ref('odoo_coopcycle_connector.product_coopcycle_service'))"/>
  </record>
</odoo>