CREATE OR REPLACE TEMPORARY TABLE {{ database_name }}.{{ schema_name }}.ROW_MD5_{{ chunk_id }}_{{ table_id }}(
    {% for index_column in index_column_collection %}
          "{{ index_column.name }}" {{ index_column.data_type }},
    {% endfor %}
    ROW_MD5 VARCHAR(32)
) AS SELECT
        {% for index_column in index_column_collection %}
          "{{ index_column.name }}_IDX",
        {% endfor %}
        UPPER(MD5(ROW_CONCAT_VALUES))
     FROM {{ database_name }}.{{ schema_name }}.ROW_CONCATENATED_{{ chunk_id }}_{{ table_id }};
