INSERT INTO CHUNKS_MD5_{{ normalized_fully_qualified_name }}_{{ table_id }}
SELECT
    '{{ chunk_id }}',
    UPPER(MD5(LISTAGG(CAST(ROW_MD5 AS VARCHAR(MAX)), '') WITHIN GROUP (ORDER BY {% for index_column in index_column_collection %}"{{ index_column.name }}"{% if not loop.last %}, {% endif %}{% endfor %})))
FROM ROW_MD5_{{ chunk_id }}_{{ table_id }}; 