{
  "mcpServers": {
    "sequential-thinking": {
      "command": "/home/linuxbrew/.linuxbrew/bin/mcp-server-sequential-thinking"
    },
    "git": {
      "command": "/home/jgodau/.local/bin/mcp-server-git",
      "args": [
        "-r",
        "/home/jgodau/work/personal/gdal-mcp"
      ],
      "env": {
        "PATH": "/home/jgodau/.local/bin:/home/linuxbrew/.linuxbrew/bin:/usr/bin:/bin",
        "GIT_DISCOVERY_ACROSS_FILESYSTEM": "1"
      }
    },
    "context7": {
      "command": "/home/linuxbrew/.linuxbrew/bin/context7-mcp",
      "args": []
    },
    "filesystem": {
      "command": "/home/linuxbrew/.linuxbrew/bin/mcp-server-filesystem",
      "args": [
        "/home/jgodau/work/personal/gdal-mcp"
      ]
    },
    "conport": {
      "command": "/home/jgodau/.local/bin/conport-mcp",
      "args": [
        "--mode",
        "stdio",
        "--workspace_id",
        "/home/jgodau/work/personal/gdal-mcp",
        "--log-file",
        "/home/jgodau/.conport/conport.log",
        "--log-level",
        "INFO"
      ]
    },
    "codex": {
      "command": "codex",
      "args": [
        "mcp",
        "-c",
        "working_dir\u003d\"/home/jgodau/work/personal/gdal-mcp\""
      ],
      "env": {
        "OPENAI_API_KEY": "",
        "OPENAI_ORG_ID": "",
        "OPENAI_PROJECT": "",
        "CODEX_HOME": "/home/jgodau/.codex",
        "NO_COLOR": "1"
      }
    },
    "serena": {
      "command": "/usr/local/bin/uvx",
      "args": [
        "--from", "git+https://github.com/oraios/serena",
        "serena", "start-mcp-server",
        "--context", "ide-assistant",
        "--project", "/home/jgodau/work/personal/gdal-mcp"
      ]
    }
  }
}