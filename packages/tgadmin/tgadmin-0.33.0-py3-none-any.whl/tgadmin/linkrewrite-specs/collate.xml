<rw:importSpec xmlns:rw="http://textgrid.info/import"
	xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://textgrid.info/import ../../xsd/textgrid-import-2010.xsd "
	xmlns:cs="http://textgrid.info/namespaces/collation/collationSet"
	xmlns:eq="http://textgrid.info/namespaces/collation/tokenModel">

	<rw:xmlConfiguration xml:id="collation-set" description="Collation Set">
		<rw:element name="cs:regularisation" method="token"/>
		<rw:any-element name="any" method="none">
		  <rw:attribute name="uri" method="full"/>
		</rw:any-element>
	</rw:xmlConfiguration>

	<rw:xmlConfiguration xml:id="collation-equivs" description="Collation Regularisation">
		<rw:any-element name="any" method="none">
			<rw:attribute name="uri" method="full"/>
		</rw:any-element>
	</rw:xmlConfiguration>

</rw:importSpec>
