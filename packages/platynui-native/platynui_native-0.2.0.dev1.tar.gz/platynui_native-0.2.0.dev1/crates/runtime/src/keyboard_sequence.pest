sequence = { SOI ~ segment* ~ EOI }

segment = { shortcut | text }

// plain text fragments outside shortcut blocks
text = @{ (escape | (!("<" | "\\") ~ ANY))+ }

// shortcut block like <Ctrl+Alt+T Ctrl+C>
shortcut = { "<" ~ combination ~ (shortcut_separator ~ combination)* ~ shortcut_separator* ~ ">" }

shortcut_separator = _{ command_space+ }

combination = { key ~ ("+" ~ key)* }

command_space = _{ " " | "\t" | "\n" | "\r" }

key = @{ ("_" | unicode_alnum | escape)+ }

escape = @{ "\\" ~ (escape_unicode | escape_hex | escape_special | escape_generic) }

escape_unicode = { "u" ~ hex_digit ~ hex_digit ~ hex_digit ~ hex_digit }

escape_hex = { "x" ~ hex_digit ~ hex_digit }

escape_special = { "<" | ">" | "\\" }

escape_generic = { !("\n" | "\r") ~ ANY }

hex_digit = { '0'..'9' | 'A'..'F' | 'a'..'f' }

