{
  "schema_version": "1.0",
  "defaults": {
    "flags": [
      "IGNORECASE",
      "MULTILINE"
    ],
    "min_rating": 1.0,
    "max_rating": 5.0
  },
  "rules": [
    {
      "id": "out_of_5_any",
      "pattern": "(?:\\(|\\[)?\\s*((?:[1-5](?:[.,]5)?)|(?:[1-4]\\s?(?:¬Ω|1/2)))\\s*/\\s*5\\s*(?:\\)|\\])?",
      "example": "4,5/5 mantap",
      "type": "extract",
      "value_group": 1,
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 101
    },
    {
      "id": "out_of_any_scale",
      "pattern": "\\b(\\d+(?:[.,]\\d+)?)\\s*/\\s*(\\d+)\\b",
      "example": "8/10 bintang",
      "type": "extract",
      "value_group": 1,
      "scale_denominator_group": 2,
      "postprocess": {
        "replace": {
           ",": "."
          }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 100
    },
    {
      "id": "bintang_any",
      "trigger_pattern": "\\b(?:(?:kasih|beri|dapat|ngasih)\\s*)?(?:bintang(?:nya|ny|\\snya)?\\s*((?:\\d+(?:[.,]\\d+)?)|(?:[1-4]\\s?(?:¬Ω|1/2)))(?:\\+{1,2})?|([1-5])\\s*bintang|(satu|dua|tiga|empat|lima)\\s*bintang|bintang\\s*(satu|dua|tiga|empat|lima))\\b",
      "pattern" : "(?:bintang(?:nya|ny|\\snya)?\\s*((?:\\d+(?:[.,]\\d+)?)|(?:[1-4]\\s?(?:¬Ω|1/2)))(?:\\+{1,2})?|([1-5])\\s*bintang|(satu|dua|tiga|empat|lima)\\s*bintang)\\b",
      "example": "kasih 5 bintang",
      "type": "extract_multi",
      "value_groups": [
        1,
        2
      ],
      "word_groups": [
        3
      ],
      "word_map": {
        "satu": 1,
        "dua": 2,
        "tiga": 3,
        "empat": 4,
        "lima": 5
      },
      "supports_half": true,
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "treat_many_as_max": true,
      "priority": 98
    },
    {
      "id": "number_of_five",
      "pattern": "\\b([1-5])\\s*(?:of|out\\s+of|dr|dari)\\s*5\\b",
      "example": "4 of 5 for design",
      "type": "extract",
      "value_group": 1,
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 96
    },
    {
      "id": "rating_word_number",
      "pattern": "\\b(rating|rate|nilai|skor|score|grade)\\s*[:\\-]?\\s*([1-5](?:[.,]5)?)\\b",
      "example": "rating: 5",
      "type": "extract",
      "value_group": 2,
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 94
    },
    {
      "id": "stars_word_any",
      "pattern": "\\b((?:[1-5])|(?:satu|dua|tiga|empat|lima))\\s*stars?\\b",
      "example": "4 stars overall",
      "type": "extract_or_map",
      "value_group": 1,
      "word_groups": [
        1
      ],
      "word_map": {
        "satu": 1,
        "dua": 2,
        "tiga": 3,
        "empat": 4,
        "lima": 5
      },
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 92
    },
    {
      "id": "emoji_stars_sequence",
      "pattern": "((?:‚≠ê\\uFE0F?|‚òÖ\\uFE0F?|üåü\\uFE0F?|‚ú©\\uFE0F?|‚úÆ\\uFE0F?|‚úØ\\uFE0F?){2,5})",
      "type": "emoji_or_mult",
      "emojis": ["‚≠ê", "‚òÖ", "üåü", "‚ú©", "‚úÆ", "‚úØ"],
      "example": "Kualitasnya ‚≠ê‚≠ê‚≠ê‚≠ê top!",
      "min_count": 2,
      "max_count": 5,
      "priority": 91
    },
    {
      "id": "emoji_stars_multiplied",
      "pattern": "(?:‚≠ê\\uFE0F?|‚òÖ\\uFE0F?)\\s*[xX√ó]\\s*([1-5])",
      "example": "Keren banget ‚≠ê x5, mantap!",
      "type": "emoji_or_mult",
      "mult_group": 1,
      "priority": 89,
      "emojis": ["‚≠ê", "‚òÖ"]
    }
  ],
  "blacklist": [
    {
      "id": "star_bullet_single",
      "pattern": "^\\s*[‚òÖ‚≠ê]\\s*$",
      "reason": "Bullet list (satu bintang di awal baris), bukan rating."
    }
  ]
}