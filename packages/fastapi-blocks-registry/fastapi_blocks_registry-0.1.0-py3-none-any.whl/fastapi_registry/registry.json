{
  "auth": {
    "name": "Authentication",
    "description": "JWT-based authentication with refresh tokens and user management",
    "version": "1.0.0",
    "path": "modules/auth",
    "dependencies": [
      "PyJWT>=2.10.0",
      "passlib[bcrypt]>=1.7.4",
      "bcrypt>=4.0.1",
      "python-multipart>=0.0.20"
    ],
    "python_version": ">=3.12",
    "env": {
      "SECRET_KEY": "change-me-32-chars-minimum-length",
      "JWT_ALGORITHM": "HS256",
      "ACCESS_TOKEN_EXPIRES_MINUTES": "30",
      "REFRESH_TOKEN_EXPIRES_DAYS": "7",
      "PASSWORD_RESET_TOKEN_EXPIRES_HOURS": "1"
    },
    "settings_class": "AuthSettings",
    "router_prefix": "/api/v1/auth",
    "tags": ["Authentication"],
    "author": "FastAPI Blocks Registry",
    "repository": "https://github.com/yourusername/fastapi-blocks-registry"
  },
  "users": {
    "name": "User Management",
    "description": "CRUD endpoints for user management with role-based access control",
    "version": "1.0.0",
    "path": "modules/users",
    "dependencies": [],
    "python_version": ">=3.12",
    "env": {},
    "settings_class": "UserSettings",
    "router_prefix": "/api/v1/users",
    "tags": ["Users"],
    "author": "FastAPI Blocks Registry",
    "repository": "https://github.com/yourusername/fastapi-blocks-registry"
  }
}
