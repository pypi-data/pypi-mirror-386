rule infer
  command = /drone/src/.venv/bin/python -m pytype.main --imports_info $imports --module-name $module --platform linux -V 3.10 -o $out --no-report-errors --nofail --quick $in
  description = infer $module
rule check
  command = /drone/src/.venv/bin/python -m pytype.main --imports_info $imports --module-name $module --platform linux -V 3.10 -o $out --analyze-annotated --nofail --quick $in
  description = check $module
build /drone/src/.pytype/pyi/lithi/core/core.pyi: check /drone/src/src/lithi/core/core.py
