ExecStart=/home/rfg/tessdb-server-ng/.venv/bin/tess-db-alarms --log-file /home/rfg/tessdb-server-ng/tdbalarm.log --file /var/log/tessdb.log
EnvironmentFile=/home/rfg/tessdb-server-ng/.env
