<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field
            name="inherit_id"
            ref="stock_available_to_promise_release.res_config_settings_view_form_stock"
        />
        <field name="arch" type="xml">
            <xpath expr="//block[@name='stock_allocation_release']" position="inside">
                <setting>
                    <field
                        name="stock_sale_report_display_is_available"
                        string="Show Available column in SO and quotation reports"
                    />
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
