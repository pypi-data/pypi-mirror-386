{
  "$defs": {
    "EtiqRCA": {
      "description": "Class representing the DAG indicating how the RCA is to be run.\n    ",
      "properties": {
        "graph": {
          "items": {
            "maxItems": 3,
            "minItems": 3,
            "prefixItems": [
              {
                "type": "string"
              },
              {
                "type": "string"
              },
              {
                "anyOf": [
                  {
                    "type": "boolean"
                  },
                  {
                    "type": "null"
                  }
                ]
              }
            ],
            "type": "array"
          },
          "title": "Graph",
          "type": "array"
        },
        "default_entrypoint": {
          "title": "Default Entrypoint",
          "type": "string"
        }
      },
      "required": [
        "graph",
        "default_entrypoint"
      ],
      "title": "EtiqRCA",
      "type": "object"
    },
    "EtiqTests": {
      "description": "Class to represent the tests and their config to be run at each node of the RCA.\n    ",
      "properties": {
        "nodes": {
          "additionalProperties": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "title": "Nodes",
          "type": "object"
        },
        "config": {
          "additionalProperties": {
            "type": "object"
          },
          "title": "Config",
          "type": "object"
        }
      },
      "required": [
        "nodes",
        "config"
      ],
      "title": "EtiqTests",
      "type": "object"
    }
  },
  "description": "Implementation agnostic class representing the RCA.\n    ",
  "properties": {
    "tests": {
      "$ref": "#/$defs/EtiqTests"
    },
    "rca": {
      "$ref": "#/$defs/EtiqRCA"
    }
  },
  "required": [
    "tests",
    "rca"
  ],
  "title": "RCAConfig",
  "type": "object"
}
