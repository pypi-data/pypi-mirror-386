[MAIN]
jobs = 0
suggestion-mode = yes
disable = missing-module-docstring,
          missing-class-docstring,
          missing-function-docstring,
          too-few-public-methods,
          too-many-arguments,
          too-many-positional-arguments,
          protected-access

[BASIC]
good-names = i,j,k,m,n,s,fp

[FORMAT]
expected-line-ending-format = LF
max-line-length = 160
indent-string = '    '
