{
    "source-release": {
        "pattern_fstring": "^(?:-(?P<debug>Debug))?\\.(?P<format>({'|'.join((key.replace('.', r'\\.') for key in format.keys()))}))$",
        "map": {
            "format": {
                "tar.xz": "tarball_lzma",
                "tgz": "tarball_deflate",
                "tar.bz2": "tarball_bzip2",
                "tar.gz": "tarball_deflate",
                "zip": "archive_deflate"
            },
            "debug": {"Debug": true}
        }
    },
    "macos-release": {
        "pattern_fstring": "^(?P<post_build>post\\d+)?(?:-(?P<options>no-lto))?(?:(-|_)macosx?(?P<min_macos>(?:\\d+(?:\\.\\d+)?)?-?(?:\\d\\d\\d\\d-\\d\\d?-\\d\\d?)?)?)?(?:_(?P<revision>rev\\d+))?\\.(?P<format>({'|'.join((key.replace('.', r'\\.') for key in format.keys()))}))$",
        "map": {
            "format": {
                "pkg": "installer_package",
                "dmg": "disk_image"
            }
        }
    },
    "windows-release": {
        "pattern_fstring": "^(?:(-|.)(?P<post_build>(\\d+|post\\d+)))?(?P<free_threaded>t)?(?:-(?P<test>test))?(?:-(?P<embed>(embeddable|embed)))?(?:(-|\\.)(?P<arch>({'|'.join(arch.keys())})))?(?:-(?P<webinstall>webinstall))?\\.(?P<format>({'|'.join((key.replace('.', r'\\.') for key in format.keys()))}))$",
        "pattern_defaults": {
            "arch": "win32"
        },
        "map": {
            "format": {
                "exe": "executable",
                "msi": "installation_packages",
                "zip": "archive_deflate"
            },
            "arch": {
                "amd64": "amd64",
                "arm64": "arm64",
                "win32": "win32",
                "x86": "win32",
                "ia64": "ia64"
            },
            "webinstall": {"webinstall": true},
            "free_threaded": {"t": true},
            "test": {"test": true},
            "embed": {"embed": true, "embeddable": true}
        }
    },
    "windows-support": {
        "pattern_fstring": "^(?:(-|\\.)(?P<arch>(amd64|arm64|win32|x86|ia64)))?(?:-(?P<pdb>pdb))\\.(?P<format>(exe|msi|zip))$"
    },
    "android-release": {
        "pattern_fstring": "^-(?P<arch>({'|'.join(arch.keys())}))-linux-android\\.(?P<format>({'|'.join((key.replace('.', r'\\.') for key in format.keys()))}))$",
        "map": {
            "format": {
                "tar.gz": "tarball_deflate"
            },
            "arch": {
                "aarch64": "arm64",
                "x86_64": "x86_64"
            }
        }
    }
}