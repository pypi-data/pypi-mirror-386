<?xml version="1.0" encoding="utf-8" ?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="TaxInvoiceBulk">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="TIN" type="xs:unsignedLong" />
        <xs:element name="ListOfTaxInvoice">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TaxInvoice">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="TaxInvoiceDate" type="xs:date" />
                    <xs:element name="TaxInvoiceOpt" type="xs:string" />
                    <xs:element name="TrxCode" type="xs:unsignedByte" />
                    <xs:element name="AddInfo" type="xs:string" />
                    <xs:element name="CustomDoc" type="xs:integer" />
                    <xs:element name="CustomDocMonthYear" type="xs:unsignedInt" />
                    <xs:element name="RefDesc" />
                    <xs:element name="FacilityStamp" type="xs:string" />
                    <xs:element name="SellerIDTKU" type="xs:integer" />
                    <xs:element name="BuyerTin" type="xs:unsignedLong" />
                    <xs:element name="BuyerDocument" type="xs:string" />
                    <xs:element name="BuyerCountry" type="xs:string" />
                    <xs:element name="BuyerDocumentNumber" />
                    <xs:element name="BuyerName" />
                    <xs:element name="BuyerAdress" />
                    <xs:element name="BuyerEmail" type="xs:string" />
                    <xs:element name="BuyerIDTKU" type="xs:integer" />
                    <xs:element name="ListOfGoodService">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="GoodService">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Opt" type="xs:string" />
                                <xs:element name="Code" type="xs:unsignedByte" />
                                <xs:element name="Name" type="xs:string" />
                                <xs:element name="Unit" type="xs:string" />
                                <xs:element name="Price" type="xs:unsignedShort" />
                                <xs:element name="Qty" type="xs:unsignedByte" />
                                <xs:element name="TotalDiscount" type="xs:unsignedInt" />
                                <xs:element name="TaxBase" type="xs:unsignedInt" />
                                <xs:element name="OtherTaxBase" type="xs:unsignedInt" />
                                <xs:element name="VATRate" type="xs:unsignedByte" />
                                <xs:element name="VAT" type="xs:unsignedInt" />
                                <xs:element name="STLGRate" type="xs:unsignedByte" />
                                <xs:element name="STLG" type="xs:unsignedInt" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
