# FEATURES

feature aalt { # ALL ALTERNATES
    feature locl;
    feature ordn;
    feature numr;
    feature dnom;
    feature sinf;
    feature sups;
    feature zero;
    feature onum;
    feature lnum;
    feature tnum;
    feature pnum;
    feature c2sc;
    feature smcp;
    feature case;
    feature hist;
    feature ss01;
    feature ss02;
    feature ss03;
    feature ss04;
    feature ss05;
    feature ss06;
} aalt;

feature ornm {
    lookup arrows_all {
        sub @arrows.from by @arrows.to;
    } arrows_all;
} ornm ;

feature cpsp { # Capital Spacing
    pos @uc.all  <5 0 10 0>;
} cpsp;

feature locl { # Localized Forms
    script latn;
    language AZE; # Azeri
        sub i by idotaccent;
    language CRT; # Crimean Tatar
        sub i by idotaccent;
    language MOL; # Moldavian
        sub [Scedilla scedilla.sc scedilla] by [Scommaaccent scommaaccent.sc scommaaccent];
    language NLD; #Dutch
		sub iacute j' by jacute;
		sub iacute.sc j.sc' by jacute.sc;
		sub Iacute J' by Jacute;
    language ROM; # Romanian
        sub [Scedilla scedilla.sc scedilla] by [Scommaaccent scommaaccent.sc scommaaccent];
    language TRK; # Turkish        
        sub i by idotaccent;
    script cyrl;
    language SRB; # Serbian
        sub [uni0431 uni0433 uni0434 uni043F uni0442] by [uni0431.locl uni0433.locl uni0434.locl uni043F.locl uni0442.locl];
    language BGR; # Bulgarian
        sub [uni0414 uni0416 uni041A uni041B uni0432 uni0433 uni0434 uni0436 uni0437 uni0438 uni0439 uni043A uni043B uni043D uni043F uni0442 uni0446 uni0447 uni0448 uni0449 uni044A uni044C uni044E] by [uni0414.bgr uni0416.bgr uni041A.bgr uni041B.bgr uni0432.bgr uni0433.bgr uni0434.bgr uni0436.bgr uni0437.bgr uni0438.bgr uni0439.bgr uni043A.bgr uni043B.bgr uni043D.bgr uni043F.bgr uni0442.bgr uni0446.bgr uni0447.bgr uni0448.bgr uni0449.bgr uni044A.bgr uni044C.bgr uni044E.bgr];

} locl;

feature ss05 {
    featureNames { 
        name "Serbian Localized Forms"; # Windows English
        name 1 0 0 "Serbian Localized Forms"; # Mac English
    };
    sub [uni0431 uni0433 uni0434 uni043F uni0442] by [uni0431.locl uni0433.locl uni0434.locl uni043F.locl uni0442.locl];
} ss05;

feature ss06 {
    featureNames { 
        name "Bulgarian Localized Forms"; # Windows English
        name 1 0 0 "Bulgarian Localized Forms"; # Mac English
    };
    sub [uni0414 uni0416 uni041A uni041B uni0432 uni0433 uni0434 uni0436 uni0437 uni0438 uni0439 uni043A uni043B uni043D uni043F uni0442 uni0446 uni0447 uni0448 uni0449 uni044A uni044C uni044E] by [uni0414.bgr uni0416.bgr uni041A.bgr uni041B.bgr uni0432.bgr uni0433.bgr uni0434.bgr uni0436.bgr uni0437.bgr uni0438.bgr uni0439.bgr uni043A.bgr uni043B.bgr uni043D.bgr uni043F.bgr uni0442.bgr uni0446.bgr uni0447.bgr uni0448.bgr uni0449.bgr uni044A.bgr uni044C.bgr uni044E.bgr];
} ss06;

feature ordn { # Ordinals
    lookup ordn_base {
        sub @figs.default [O o.sc o]' by ordmasculine;
        sub @figs.default [A a.sc a]' by ordfeminine;
    } ordn_base;
    lookup ordn_numero {
        sub N [O o o.sc degree ordmasculine] by uni2116;
        sub n [O o o.sc degree ordmasculine] by uni2116;
        sub n.sc [O o o.sc degree ordmasculine] by uni2116;
    } ordn_numero;
} ordn;
    
