feature numr { # numerator
    lookup numr_all {
        sub @figs.default by @figs.numerator;
    } numr_all;
} numr;

feature dnom { # denominator
    lookup dnom_all {
        sub @figs.default by @figs.denominator;
    } dnom_all;
} dnom;

feature sinf { # inferior
    lookup sups_all {
        sub @figs.default by @figs.inferior;
    } sups_all;
} sinf;

feature sups { # superior
    lookup sups_figs {
        sub @figs.default by @figs.superior;
    } sups_figs;
    lookup sups_letters {
        sub @sup_from by @sup_to;
        sub idotaccent by i.superior;
    } sups_letters;
} sups;

feature zero { # zero slash
    lookup zero_all {
        sub zero by zero.zero;
    } zero_all;
} zero;

feature onum { # old style figures
    lookup onum_all {
        sub [@figs.lt @figs.lp] by [@figs.ot @figs.op];
        sub [@currency.lt @currency.lp] by [@currency.ot @currency.op];
    } onum_all;
} onum;

feature lnum { # lining figures
    lookup lnum_all {
        sub [@figs.ot @figs.op] by [@figs.lt @figs.lp];
        sub [@currency.ot @currency.op] by [@currency.lt @currency.lp];
    } lnum_all;
} lnum;

feature tnum { # tabular figures
    lookup tnum_all {
        sub [@figs.lp @figs.op] by [@figs.lt @figs.ot];
        sub [@currency.lp @currency.op] by [@currency.lt @currency.ot];
    } tnum_all;
} tnum;

feature pnum { # proportional figures
    lookup pnum_all {
        sub [@figs.lt @figs.ot] by [@figs.lp @figs.op];
        sub [@currency.lt @currency.ot] by [@currency.lp @currency.op];
    } pnum_all;
} pnum;

feature c2sc { # caps to small caps
    lookup c2sc_letters {
          sub @uc by @sc;
          sub @uc_cyr by @sc_cyr_uc;
          sub @lc by @sc;
          sub @lc_cyr by @sc_cyr_lc;
          sub @lc.punct_sc by @sc.punct;
          sub Germandbls by germandbls.sc;
          sub Idotaccent by idotaccent.sc;
      } c2sc_letters;
      lookup onum_all;
} c2sc;

feature smcp { # small caps
    lookup smcp_letters {
          sub @lc by @sc;
          sub @lc.punct_sc by @sc.punct;
          sub idotless by i.sc;
          sub idotaccent by idotaccent.sc;
          sub germandbls by germandbls.sc;
          sub @lc_cyr by @sc_cyr_lc;
      } smcp_letters;
      lookup onum_all;
} smcp;

feature case { # Case-sensitive forms
    lookup case {
        sub @lc by @uc;
    } case; 
    lookup case_exceptions {
         sub [germandbls longs] by [Germandbls S]; 
         sub [uni01C5 uni01C8 uni01CB] by [uni01C4 uni01C7 uni01CA];
         sub idotaccent by Idotaccent;
         sub idotless by I;
     } case_exceptions;
    lookup case_punctuation {
        sub @lc.punct_uc by @uc.punct;
    } case_punctuation; 
    lookup lnum_all;
} case;

feature hist { # Historical Forms
    lookup hist_all {
        sub s' @lc.all by longs;
    } hist_all;
} hist;

feature liga {
    lookup liga_all {
        sub f f b by f_f_b ;
        sub f f h by f_f_h ;
        sub f f i by f_f_i ;
        sub f f j by f_f_j ;
        sub f f k by f_f_k ;
        sub f f l by f_f_l ;
        sub f f t by f_f_t ;
        sub f b by f_b ;
        sub f f by f_f ;
        sub f h by f_h ;
        sub f i by fi ;
        sub f j by f_j ;
        sub f k by f_k ;
        sub f l by fl ;
        sub f t by f_t ;
    } liga_all ;
} liga ;

feature dlig {
    lookup dlig_all {
        sub c h by c_h ;
        sub c t by c_t ;
        sub s t by s_t ;
        sub s p by s_p ;
        sub T h by T_h ;
    } dlig_all ;
} dlig ;

feature ss01 { # Open circled figures
    featureNames {
        name "Open circled figures"; # Windows English
        name 1 0 0 "Open circled figures"; # Mac English
    };
    lookup opencirc_01 {
        sub one zero by uni2789;
    } opencirc_01;
    lookup opencirc_02 {
        sub @figs.default by @figs.opencirc;
    } opencirc_02;
} ss01;

feature ss02 { # close circled figures
    featureNames {
        name "Closed circled figures"; # Windows English
        name 1 0 0 "Closed circled figures"; # Mac English
    };
    lookup closecirc_01 {
        sub one zero by uni2793;
    } closecirc_01;
    lookup closecirc_02 {
        sub @figs.default by @figs.closecirc;
    } closecirc_02;
} ss02;

feature ss03 { #Arrows
    featureNames {
        name "Arrows"; # Windows English
        name 1 0 0 "Arrows"; # Mac English
    };
    lookup arrows_all;
} ss03;

feature ss04 { #Dingbats
    featureNames { 
        name "Dingbats"; # Windows English
        name 1 0 0 "Dingbats"; # Mac English
    };
    lookup dingbats_all {
        sub @dingbats.from by @dingbats.to;
    } dingbats_all;
} ss04;
