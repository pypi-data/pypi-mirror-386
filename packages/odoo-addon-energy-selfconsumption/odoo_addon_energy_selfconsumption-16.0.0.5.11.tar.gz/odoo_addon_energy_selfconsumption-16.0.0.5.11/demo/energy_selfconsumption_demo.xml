<!--
Energy Self-consumption Demo Data
=================================

This file contains demo data for the energy self-consumption module that creates
sample projects, inscriptions, supply points, and distribution tables for testing
and demonstration purposes.

Main demo data includes:
- Two self-consumption projects (Barri Sants and Coòpolis)
- Five inscriptions with associated partners
- Five supply points with different locations
- Distribution table with coefficient assignments
- Complete workflow demonstration from draft to activation
-->
<odoo>

  <!-- ========================================== -->
  <!-- SELFCONSUMPTION JOURNAL                 -->
  <!-- ========================================== -->
  <record id="selfconsumption_journal_community_1_demo" model="account.journal">
    <field name="name">Autoconsumo Fotovoltaico Compartido</field>
    <field name="type">sale</field>
    <field name="code">AFC</field>
    <field
      name="company_id"
      eval="obj(ref('energy_communities.energy_community_company_1_wizard')).new_company_id"
      model="account.multicompany.easy.creation.wiz"
    />
    <!-- Atention if community 1 id != 5 -->
    <field
      name="default_account_id"
      eval="obj(ref('l10n_es.5_account_common_7050'))"
      model="account.account"
    />
    <field name="refund_sequence" eval="True"/>
  </record>

  <function
    name="write_with_company"
    model="product.category"
  >
    <value
      eval="ref('product_category_selfconsumption_pack')"
      model="product.category"
    />
    <value
      eval="obj(ref('energy_communities.energy_community_company_1_wizard')).new_company_id"
      model="account.multicompany.easy.creation.wiz"
    />
    <value
      eval="{
        'service_invoicing_sale_journal_id': obj(ref('selfconsumption_journal_community_1_demo')).id,
        'service_invoicing_purchase_journal_id': obj(ref('selfconsumption_journal_community_1_demo')).id
      }"
      model="account.journal"
    />
  </function>

