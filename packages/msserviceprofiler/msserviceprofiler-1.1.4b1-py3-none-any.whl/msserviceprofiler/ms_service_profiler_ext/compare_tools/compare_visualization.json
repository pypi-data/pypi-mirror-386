            "queryText": "SELECT\n    CAST(FLOOR(value / ${batch_group}) * ${batch_group} AS VARCHAR) || '-' || \n    CAST(FLOOR(value / ${batch_group}) * ${batch_group} + ${batch_group} AS VARCHAR) AS group_name,\n    \n    SUM(CASE WHEN type_metric = 'Prefill_a' THEN 1 ELSE 0 END) AS Prefill_a,\n    SUM(CASE WHEN type_metric = 'Prefill_b' THEN 1 ELSE 0 END) AS Prefill_b,\n    SUM(CASE WHEN type_metric = 'Decode_a' THEN 1 ELSE 0 END) AS Decode_a,\n    SUM(CASE WHEN type_metric = 'Decode_b' THEN 1 ELSE 0 END) AS Decode_b\nFROM (\n    SELECT 'Prefill_a' AS type_metric, batch_size_a AS value\n    FROM modelExec\n    UNION ALL\n    SELECT 'Prefill_b' AS type_metric, batch_size_b AS value\n    FROM modelExec\n    UNION ALL\n    SELECT 'Decode_a' AS type_metric, batch_size_a AS value\n    FROM modelExec\n    WHERE batch_type = 'Decode'\n    UNION ALL\n    SELECT 'Decode_b' AS type_metric, batch_size_b AS value\n    FROM modelExec\n    WHERE batch_type = 'Decode'\n) AS metrics\nWHERE value IS NOT NULL\nGROUP BY \n    group_name\nORDER BY \n    CAST(group_name AS INTERVAL) ASC;",
            "queryType": "table",
            "rawQueryText": "SELECT\n    CAST(FLOOR(value / ${batch_group}) * ${batch_group} AS VARCHAR) || '-' || \n    CAST(FLOOR(value / ${batch_group}) * ${batch_group} + ${batch_group} AS VARCHAR) AS group_name,\n    \n    SUM(CASE WHEN type_metric = 'Prefill_a' THEN 1 ELSE 0 END) AS Prefill_a,\n    SUM(CASE WHEN type_metric = 'Prefill_b' THEN 1 ELSE 0 END) AS Prefill_b,\n    SUM(CASE WHEN type_metric = 'Decode_a' THEN 1 ELSE 0 END) AS Decode_a,\n    SUM(CASE WHEN type_metric = 'Decode_b' THEN 1 ELSE 0 END) AS Decode_b\nFROM (\n    SELECT 'Prefill_a' AS type_metric, batch_size_a AS value\n    FROM modelExec\n    UNION ALL\n    SELECT 'Prefill_b' AS type_metric, batch_size_b AS value\n    FROM modelExec\n    UNION ALL\n    SELECT 'Decode_a' AS type_metric, batch_size_a AS value\n    FROM modelExec\n    WHERE batch_type = 'Decode'\n    UNION ALL\n    SELECT 'Decode_b' AS type_metric, batch_size_b AS value\n    FROM modelExec\n    WHERE batch_type = 'Decode'\n) AS metrics\nWHERE value IS NOT NULL\nGROUP BY \n    group_name\nORDER BY \n    CAST(group_name AS INTERVAL) ASC;",
            "refId": "A",
            "timeColumns": [
              "time",
              "ts"
            ]
          }
