{
  "name": "@bovine/comments-api",
  "version": "0.0.1-3",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.gen.ts"
  },
  "author": {
    "name": "Helge"
  },
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.25.0",
    "@types/node": "^24.3.3",
    "typedoc": "^0.27.6"
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.83.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "docs": "typedoc --entryPoints src/sdk.gen.ts --entryPoints src/types.gen.ts  --entryPoints src/client.gen.ts --entryPoints src/index.ts  --out docs",
    "generate": "openapi-ts   -i ../../docs/assets/schemas/api.json   -o src   -c @hey-api/client-fetch",
    "build": "npm run generate; npm run docs"
  },
  "packageManager": "yarn@4.6.0"
}