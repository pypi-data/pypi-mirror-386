{% load cfdi_tags %}
<cfdi:Addenda>
    <Addenda_ZF_Detecno>
        <Version>1.0</Version>
        <Moneda>{{ moneda }}</Moneda>
        <IDFactura>{{ id_factura }}</IDFactura>
        <Conceptos>{% for concepto in conceptos %}
            <Concepto NumPosicion="{{ concepto.num_posicion }}" NumOc="{{ concepto.num_oc }}" cantidad="{{ concepto.cantidad }}" valorUnitario="{{ concepto.valor_unitario }}" importe="{{ concepto.importe }}"/>
        {% endfor %}</Conceptos>
    </Addenda_ZF_Detecno>
</cfdi:Addenda>