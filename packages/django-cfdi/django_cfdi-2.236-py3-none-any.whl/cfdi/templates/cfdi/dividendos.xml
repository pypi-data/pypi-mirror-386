<?xml version="1.0" encoding="UTF-8"?>
<retenciones:Retenciones xsi:schemaLocation="http://www.sat.gob.mx/esquemas/retencionpago/2 http://www.sat.gob.mx/esquemas/retencionpago/2/retencionpagov2.xsd http://www.sat.gob.mx/esquemas/retencionpago/1/dividendos http://www.sat.gob.mx/esquemas/retencionpago/1/dividendos/dividendos.xsd"
	xmlns:dividendos="http://www.sat.gob.mx/esquemas/retencionpago/1/dividendos" 
    Version="2.0" FolioInt="{{ dividendo.FolioInt }}" 
    NoCertificado="{{ dividendo.NoCertificado }}" 
    Sello="X" Certificado="{{ dividendo.Certificado }}" 
    FechaExp="{{ dividendo.FechaExp }}" LugarExpRetenc="{{ dividendo.LugarExpRetenc }}" CveRetenc="{{ dividendo.CveRetenc }}"
    xmlns:retenciones="http://www.sat.gob.mx/esquemas/retencionpago/2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <retenciones:Emisor RfcE="{{ dividendo.Emisor.Rfc }}" NomDenRazSocE="{{ dividendo.Emisor.Nombre }}" RegimenFiscalE="{{ dividendo.Emisor.RegimenFiscal }}" />
	<retenciones:Receptor NacionalidadR="Nacional">
        <retenciones:Nacional RfcR="{{ dividendo.Receptor.Rfc }}" NomDenRazSocR="{{ dividendo.Receptor.Nombre }}" DomicilioFiscalR="{{ dividendo.Receptor.DomicilioFiscalReceptor }}" />
	</retenciones:Receptor>
	<retenciones:Periodo MesIni="{{ dividendo.MesIni }}" MesFin="{{ dividendo.MesFin }}" Ejercicio="{{ dividendo.Ejercicio }}" />
	<retenciones:Totales MontoTotOperacion="{{ dividendo.MontoTotOperacion }}" MontoTotGrav="{{ dividendo.MontoTotGrav }}" MontoTotExent="{{ dividendo.MontoTotExent }}" MontoTotRet="{{ dividendo.MontoTotRet }}">
        
		{% for impuesto in dividendo.impuestos_retenidos %}
			<retenciones:ImpRetenidos BaseRet="{{ impuesto.base }}" 
			ImpuestoRet="{{ impuesto.impuesto }}" MontoRet="{{ impuesto.monto }}" TipoPagoRet="{{ impuesto.tipo_pago }}" />
		{% endfor %}
	
	</retenciones:Totales>
	<retenciones:Complemento>
		<dividendos:Dividendos Version="1.0">
			<dividendos:DividOUtil 
				CveTipDivOUtil="{{ dividendo.CveTipDivOUtil }}" 
				MontISRAcredRetMexico="{{ dividendo.MontISRAcredRetMexico }}"
				MontISRAcredRetExtranjero="{{ dividendo.MontISRAcredRetExtranjero  }}" 
				TipoSocDistrDiv="{{ dividendo.TipoSocDistrDiv }}" 
				MontISRAcredNal="{{ dividendo.MontISRAcredNal }}" 
				MontDivAcumNal="{{ dividendo.MontDivAcumNal }}"/>
			{% if dividendo.ProporcionRem %} 
				<dividendos:Remanente ProporcionRem="{{ dividendo.ProporcionRem }}"/>
			{% endif %}
		</dividendos:Dividendos>
    </retenciones:Complemento>
</retenciones:Retenciones>