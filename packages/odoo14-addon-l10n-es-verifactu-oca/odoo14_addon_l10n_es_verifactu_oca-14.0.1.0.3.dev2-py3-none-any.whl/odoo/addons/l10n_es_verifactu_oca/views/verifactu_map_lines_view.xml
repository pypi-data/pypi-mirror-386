<odoo>
    <record id="l10n_es_verifactu_map_lines_view_tree" model="ir.ui.view">
        <field name="model">verifactu.map.line</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="code" />
                <field name="name" />
                <field
                    name="taxes"
                    widget="many2many_tags"
                    domain="[('type_tax_use', '=', 'sale')]"
                />
            </tree>
        </field>
    </record>

</odoo>
