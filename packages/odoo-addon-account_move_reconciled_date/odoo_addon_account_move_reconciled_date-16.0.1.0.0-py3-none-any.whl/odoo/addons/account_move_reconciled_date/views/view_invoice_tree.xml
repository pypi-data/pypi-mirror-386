<?xml version='1.0' encoding='UTF-8' ?>
<odoo>
    <record id="view_invoice_tree_show_reconcile_date" model="ir.ui.view">
        <field name="name">view_invoice_tree.show_reconcile_date</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_invoice_tree" />
        <field name="arch" type="xml">
            <field name="payment_state" position="after">
                <field name="payment_date" optional="hide" />
                <field name="reconcile_date" optional="hide" />
            </field>
        </field>
    </record>
</odoo>
