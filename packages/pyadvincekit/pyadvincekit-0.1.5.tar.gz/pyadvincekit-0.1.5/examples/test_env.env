# PyAdvanceKit 配置示例
PYADVINCEKIT_APP_NAME=我的自动配置应用
PYADVINCEKIT_DEBUG=true
PYADVINCEKIT_ENVIRONMENT=development

# 数据库配置
PYADVINCEKIT_DATABASE_URL=sqlite:///./auto_config_demo.db
PYADVINCEKIT_DATABASE_POOL_SIZE=5
PYADVINCEKIT_DATABASE_MAX_OVERFLOW=10

# 日志配置
PYADVINCEKIT_LOG_LEVEL=INFO
PYADVINCEKIT_LOG_FILE_ENABLED=true
PYADVINCEKIT_LOG_FILE_PATH=logs/auto_config_demo.log

# JWT配置
PYADVINCEKIT_JWT_SECRET_KEY=my-super-secret-jwt-key-for-auto-config-demo
PYADVINCEKIT_JWT_ALGORITHM=HS256
PYADVINCEKIT_JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

