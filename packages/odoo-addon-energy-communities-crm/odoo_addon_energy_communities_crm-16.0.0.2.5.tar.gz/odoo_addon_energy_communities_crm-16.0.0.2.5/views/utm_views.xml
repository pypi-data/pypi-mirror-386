<odoo>
    <record id="ce_utm_sources_form_view" model="ir.ui.view">
        <field name="name">utm.source.form.inherit</field>
        <field name="model">utm.source</field>
        <field name="inherit_id" ref="utm.utm_source_view_form" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="crm_team_id" />
            </field>
        </field>
    </record>

    <record id="ce_utm_sources_tree_view" model="ir.ui.view">
        <field name="name">utm.source.tree.inherit</field>
        <field name="model">utm.source</field>
        <field name="inherit_id" ref="utm.utm_source_view_tree" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="crm_team_id" />
            </field>
        </field>
    </record>
</odoo>
