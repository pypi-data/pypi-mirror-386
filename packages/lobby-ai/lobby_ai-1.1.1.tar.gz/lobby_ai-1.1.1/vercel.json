{
  "version": 2,
  "builds": [
    {
      "src": "doorman/web/vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/doorman/web/vercel_app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "doorman/web/vercel_app.py": {
      "maxDuration": 30
    }
  }
}