{
  "name": "@t-prompts/widgets",
  "version": "0.18.1",
  "type": "module",
  "description": "Jupyter notebook widgets for visualizing t-prompts structures",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "build:python": "node build.js",
    "prepublishOnly": "pnpm build",
    "pretest": "node build.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "visualization",
    "prompts",
    "structured-prompts",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/habemus-papadum/t-prompts.git",
    "directory": "widgets"
  },
  "dependencies": {
    "@mdit/plugin-katex": "^0.23.2",
    "katex": "^0.16.9",
    "markdown-it": "^14.0.0"
  },
  "devDependencies": {
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^1.4.0",
    "esbuild": "^0.20.0",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
