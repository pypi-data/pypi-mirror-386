<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<!--  XSchema $Id: Annotation.xsd,v 1.1 2006/03/04 21:18:35 graebe Exp $  -->

  <xs:include schemaLocation="Types.xsd"/>
  <xs:include schemaLocation="Root.xsd"/>

  <!-- subelement definition -->

  <xs:element name="relatesTo">
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="OWL"/>	
          <xs:element ref="XML"/>	
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- Root element definition -->

  <xs:element name="Annotation"> 
    <xs:complexType>
      <xs:sequence>
        <xs:element name="note" type="xs:string"/> 
        <xs:element ref="relatesTo"/> 
        <xs:group ref="RootTableEntries"/>
      </xs:sequence>
    </xs:complexType>
    <xs:attributeGroup ref="recordAttributes"/>
  </xs:element>
</xs:schema>
