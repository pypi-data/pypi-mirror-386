<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<!--  XSchema $Id: Contributor.xsd,v 1.1 2006/03/04 21:18:35 graebe Exp $  -->

  <xs:include schemaLocation="Types.xsd"/>
  <xs:include schemaLocation="Root.xsd"/>

  <!-- subelement definition -->

  <xs:element name="thePerson">
    <xs:complexType>
      <xs:element ref="OWL"/>	
    </xs:complexType>
  </xs:element>

  <!-- Root element definition -->

  <xs:element name="Contributor">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded"
	        name="hasContributed" type="xs:string"/> 
        <xs:element minOccurs="0" maxOccurs="unbounded"
	    name="hasEmail" type="xs:anyURI"/>  
        <xs:element minOccurs="0" maxOccurs="unbounded" name="hasURL"
	    type="xs:anyURI"/>  
        <xs:element name="theName" type="xs:string"/> 
        <xs:element name="theNickName" type="xs:QName"/> 
        <xs:element ref="thePerson"/> 
        <xs:element minOccurs="0" name="theStatus" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
    <xs:attribute name="id" type="xs:anyURI" use="required"/>
    <xs:attribute name="createdAt" type="xs:date" use="required"/>
  </xs:element>
</xs:schema>
