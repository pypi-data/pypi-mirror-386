<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<!--  XSchema $Id: Ideal.xsd,v 1.1 2006/03/04 21:18:35 graebe Exp $  -->

  <xs:include schemaLocation="Types.xsd"/>
  <xs:include schemaLocation="Root.xsd"/>

  <!-- subelement definition -->

  <xs:element name="theGenerators">
    <xs:complexType>
      <xs:element ref="OWL"/>	
    </xs:complexType>
  </xs:element>

  <!-- Root element definition -->

  <xs:element name="Ideal">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="hasIsolatedPrimeDegrees"
	        type="UnsignedIntList"/> 
        <xs:element minOccurs="0" name="hasIsolatedPrimeDimensions"
	        type="UnsignedIntList"/> 
        <xs:element minOccurs="0" maxOccurs="unbounded"
	        name="hasProperty" type="xs:token"/> 
        <xs:element name="theBaseDomain" type="xs:token"/>
        <xs:element minOccurs="0" name="theDegree" type="xs:unsignedInt"/>
        <xs:element minOccurs="0" name="theDimension" type="xs:unsignedInt"/>
        <xs:element ref="theGenerators"/>
        <xs:element minOccurs="0" name="theParameterList" type="VarList"/>
        <xs:element name="theVariableList" type="VarList"/>
        <xs:group ref="RootTableEntries"/>
      </xs:sequence>
      <xs:attributeGroup ref="recordAttributes"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
