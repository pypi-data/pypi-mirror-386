{
  "index_patterns": ["__SEARCH_INDEX_PREFIX__job-logs*"],
  "data_stream": {},
  "template": {
    "mappings": {
      "properties": {
        "@timestamp": { "type": "date" },
        "level": { "type": "keyword" },
        "message": { "type": "text" },
        "module": { "type": "keyword" },
        "function": { "type": "keyword" },
        "line": { "type": "integer" },
        "context": {
          "type": "object",
          "properties": {
            "job_id": { "type": "keyword" },
            "run_id": { "type": "keyword" },
            "identity_id": { "type": "keyword" }
          },
          "dynamic": true
        }
      }
    }
  }
}
