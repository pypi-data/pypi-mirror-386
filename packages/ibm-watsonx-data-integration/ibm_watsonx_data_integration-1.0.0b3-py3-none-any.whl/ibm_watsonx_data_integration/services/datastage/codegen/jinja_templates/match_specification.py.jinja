{{ var_name }} = (
    {% if imported %}
    {% if offline %}
    MatchSpecification(asset_id = "{{ asset_id }}")
    {% else %}
    sdk.get_match_specification(id = "{{ asset_id }}")
    {% endif %}
    {% else %}
    MatchSpecification(**(MatchSpecification.create_match_properties("{{ match_specification_name }}", {{mat_json}}, {{passes_json}})))
    {% if description %}
    .with_description("{{ description }}")
    {% endif %}
    {% endif %}
)
