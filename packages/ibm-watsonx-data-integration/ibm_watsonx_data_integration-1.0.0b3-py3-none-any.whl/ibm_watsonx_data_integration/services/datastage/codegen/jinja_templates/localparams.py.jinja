{{ var_name }} = (
    LocalParameters()
    {% for param in parameters %}
    {% if param["type"] == "List" %}
    .add_parameter(odbc_type = "{{ param["type"] }}", name = "{{ param["name"] }}", valid_values = {{ param["valid_values"] }}, value = {{ param["value"] }}{{ param["extras"] }})
    {% else %}
    .add_parameter(odbc_type = "{{ param["type"] }}", name = "{{ param["name"] }}", value = {{ param["value"] }}{{ param["extras"] }})
    {% endif %}
    {% endfor %}
)
{{ composer }}.use_localparams({{ var_name }})
