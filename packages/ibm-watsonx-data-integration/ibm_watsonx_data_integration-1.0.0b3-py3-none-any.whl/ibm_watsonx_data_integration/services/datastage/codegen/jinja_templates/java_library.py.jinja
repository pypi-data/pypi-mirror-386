{{ var_name }} = (
    {% if imported %}
    {% if offline %}
    JavaLibrary(asset_id = "{{ asset_id }}")
    {% else %}
    sdk.get_java_library(id = "{{ asset_id }}")
    {% endif %}
    {% else %}
    JavaLibrary(name="{{ java_library_name }}", primary_file="{{ output_path }}/attachments/{{ primary_file }}")
    {% if description %}
    .set_description("{{ description }}")
    {% endif %}
    {% for file in secondary_files %}
    .add_secondary_file("{{ output_path }}/attachments/{{ file }}")
    {% endfor %}
    {% endif %}
)
