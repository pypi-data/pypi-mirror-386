{% for field in fields[0] %}
{{ var_name }}.add_field("{{ field["type"] }}", "{{ field["name"] }}"){{ field["properties"] }}
{% endfor %}

{% for field_set in fields[1:] %}
{% for field in field_set %}
{{ var_name }}.add_field("{{ field["type"] }}", "{{ field["name"] }}"){{ field["properties"] }}
{% endfor %}
{% endfor %}
