{{ var_name }} = JobSettings(
    {% if runtime_parameters %}
    runtime_parameters = (
        RuntimeParameters()
        {% for ps, vs in runtime_parameters.value_sets.items() %}
        {% if vs %}
        .use_value_set(parameter_set="{{ ps }}", value_set="{{ vs }}")
        {% endif %}
        {% endfor %}
    ),
    {% endif %}
    {% if schedule %}
    schedule = Schedule(
        {% if schedule.schedule %}
        schedule = "{{ schedule.schedule }}",
        {% endif %}
        {% if schedule.repeat %}
        repeat = {{ schedule.repeat }},
        {% endif %}
        {% if schedule.start %}
        start = "{{ schedule.start }}",
        {% endif %}
        {% if schedule.end %}
        end = "{{ schedule.end }}"
        {% endif %}
    )
    {% endif %}
)
