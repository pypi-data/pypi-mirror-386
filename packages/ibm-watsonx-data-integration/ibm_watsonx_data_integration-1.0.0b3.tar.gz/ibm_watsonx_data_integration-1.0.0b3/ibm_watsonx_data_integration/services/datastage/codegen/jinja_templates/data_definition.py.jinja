{{ var_name }} = (
{% if imported %}
{% if offline %}
    DataDefinition(asset_id = "{{ asset_id }}")
)
{% else %}
    sdk.get_data_definition(id = "{{ asset_id }}")
)
{% endif %}
{% else %}
    DataDefinition(name="{{ data_definition_name }}")
)
{% for field in fields %}
{{ var_name }}.add_field("{{ field["type"] }}", "{{ field["name"] }}"){{ field["properties"] }}
{% endfor %}
{% for item in extended_prop_items %}
{{ var_name }}{{item}}
{% endfor %}
{% endif %}
