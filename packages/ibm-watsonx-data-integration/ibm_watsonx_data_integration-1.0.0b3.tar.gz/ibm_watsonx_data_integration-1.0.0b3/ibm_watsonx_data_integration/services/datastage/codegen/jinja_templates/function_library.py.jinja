{{ var_name }} = (
    {% if imported %}
    {% if offline %}
    FunctionLibrary(asset_id = "{{ asset_id }}")
    {% else %}
    sdk.get_function_library(id = "{{ asset_id }}")
    {% endif %}
    {% else %}
    FunctionLibrary(name="{{ function_library_name }}", library_path="{{ output_path }}/attachments/{{ library_path }}")
    {% if description %}
    .set_description("{{ description }}")
    {% endif %}
    {% endif %}
)
