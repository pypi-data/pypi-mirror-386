{% include "export/_base_export.jinja2" %}
SELECT
	media_context.image_or_object_id as media_id,
    tags.id as tag_id,
    media_to_tags.created_at as media_to_tags_created_at,
	tags.name as tag_name
FROM media_context
JOIN media_to_tags ON media_context.image_or_object_id = media_to_tags.media_id
JOIN tags ON media_to_tags.tag_id = tags.id
