{% include "export/_base_export.jinja2" %}
SELECT
    media_context.image_or_object_id as media_id,
    media_context.image_id,
    media_context.dataset_id,
    media_context.image_uri,
    media_context.original_uri as image_original_uri,
    media_context.bounding_box,
    media_context.cluster_type,
    media_context.cluster_id,
    labels.original_id as original_id,
    labels.category_id as category_id,
    labels.category_display_name as category_display_name,
    labels.type as label_type,
    labels.source
FROM media_context
JOIN labels ON media_context.image_or_object_id = labels.id
WHERE media_context.cluster_type = 'OBJECTS'