{% include "export/_base_export.jinja2" %}
SELECT  -- images
    {% include "export/_export_labels.jinja2" %}
FROM media_context
JOIN labels ON labels.image_id = media_context.image_or_object_id
WHERE
    labels.dataset_id = :dataset_id
    AND labels.bounding_box is null
    AND labels.type = 'IMAGE'
    AND media_context.cluster_type = 'IMAGES'
