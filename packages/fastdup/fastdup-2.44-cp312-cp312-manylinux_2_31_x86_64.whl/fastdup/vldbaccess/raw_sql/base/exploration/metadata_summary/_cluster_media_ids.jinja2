SELECT
    flat_similarity_clusters.dataset_id,
    image_or_object_id as media_id,
    flat_similarity_clusters.image_id,
    metadata->>'video' as video,
    flat_similarity_clusters.bounding_box as bbox,
    labels,
    vl_labels,
    issue_type_ids,
    cluster_type
FROM
    flat_similarity_clusters
    {% include "_similarity_join_filtering.jinja2" %}
WHERE
    True
    {% include "_composite_filter.jinja2" %}
