{% include "export/_base_export.jinja2" %}
SELECT
    media_context.image_or_object_id as media_id,
    media_context.image_id,
    media_context.dataset_id,
    media_context.image_uri,
    media_context.original_uri as image_original_uri,
    images.w as image_width,
    images.h as image_height,
    images.file_size as image_file_size,
    media_context.metadata,
    media_context.bounding_box,
    media_context.cluster_type,
    media_context.cluster_id
FROM media_context
JOIN images ON media_context.image_id = images.id
WHERE media_context.cluster_type = 'IMAGES'