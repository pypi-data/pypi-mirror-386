WITH
{% include "_similarity_filtering_cte.jinja2" %},
cluster_issue_ids AS (
    SELECT DISTINCT unnest(issue_type_ids) id
    FROM flat_similarity_clusters
    {% include "_similarity_join_filtering.jinja2" %}
    WHERE 1=1
    {% include "_composite_filter.jinja2" %}
)
SELECT
    id, name, severity
FROM issue_type
WHERE id IN (SELECT id FROM cluster_issue_ids)
ORDER BY id;
