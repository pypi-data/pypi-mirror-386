# Google AI API Configuration
GOOGLE_API_KEY=your-google-api-key-here

# OpenTelemetry Configuration
OTEL_SERVICE_NAME=google-ai-example
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318

# Feature Flags
