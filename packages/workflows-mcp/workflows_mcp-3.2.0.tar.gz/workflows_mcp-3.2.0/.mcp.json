{
  "mcpServers": {
    "workflows-new": {
      "command": "uvx",
      "args": [
        "--from", "workflows-mcp",
        "workflows-mcp"
      ],
      "env": {
        "WORKFLOWS_LOG_LEVEL": "INFO",
        "WORKFLOWS_TEMPLATE_PATHS": "~/my-workflows,/opt/team-workflows"
      }
    },
    "workflows": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/iulian/Seafile/qts/github.com/qtsone/mcp-workflows",
        "python",
        "-m",
        "workflows_mcp"
      ],
      "env": {
        "WORKFLOWS_LOG_LEVEL": "DEBUG",
        "WORKFLOWS_TEMPLATE_PATHS": ""
      }
    },
    "sequential": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking@latest"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    }
  }
}