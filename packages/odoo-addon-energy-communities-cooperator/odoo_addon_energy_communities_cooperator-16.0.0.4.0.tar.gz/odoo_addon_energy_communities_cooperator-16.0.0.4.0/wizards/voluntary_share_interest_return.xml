<odoo>
    <data>
        <record
      id="view_voluntary_share_interest_return_wizard_form"
      model="ir.ui.view"
    >
            <field name="name">voluntary.share.interest.return.wizard.form</field>
            <field name="model">voluntary.share.interest.return.wizard</field>
            <field name="arch" type="xml">
                <form string="Return voluntary shares interest">
                    <sheet>
                        <group>
                            <field name="company_id" required="1" invisible="1" />
                            <field name="interest" required="1" />
                            <field name="credit_account_move_id" required="1" />
                            <field name="return_line_account_id" required="1" />
                            <field name="tax_id" required="1" />
                            <field name="journal_id" required="1" />
                            <field
                name="payment_mode_id"
                required="1"
                domain="[('payment_type','=','outbound')]"
              />
                            <field name="start_date_period" required="1" />
                            <field name="end_date_period" required="1" />
                            <field name="invoice_date" required="1" />
                            <field name="invoice_date_due" required="1" />
                        </group>
                    </sheet>
                    <footer>
                        <button
              name="execute_return"
              string="Execute"
              type="object"
              class="btn-primary"
            />
                        <button
              string="Cancelar"
              class="btn-secondary"
              special="cancel"
            />
                    </footer>
                </form>
            </field>
        </record>
    <record
      model="ir.actions.act_window"
      id="voluntary_share_interest_return_wizard_action"
    >
        <field name="name">Create voluntary share interest return</field>
        <field name="res_model">voluntary.share.interest.return.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
    <menuitem
      id="voluntary_share_interest_return_wizard_menu"
      name="Create voluntary share interest return"
      action="voluntary_share_interest_return_wizard_action"
      parent="cooperator.menu_cooperator_config"
      sequence="440"
    />
    </data>
</odoo>
