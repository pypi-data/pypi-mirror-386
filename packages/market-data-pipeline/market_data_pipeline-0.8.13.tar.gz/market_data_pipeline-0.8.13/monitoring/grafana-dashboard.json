{
  "dashboard": {
    "id": null,
    "title": "Market Data Pipeline",
    "tags": ["market-data", "pipeline"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Pipeline Throughput",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mdp_source_items_total[1m])",
            "legendFormat": "Ticks/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Bars Generated",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(operator_bars_out_total[1m])",
            "legendFormat": "Bars/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Queue Depth",
        "type": "gauge",
        "targets": [
          {
            "expr": "mdp_sink_queue_depth",
            "legendFormat": "Sink Queue Depth"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Commit Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mdp_sink_commit_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(mdp_sink_commit_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mdp_sink_batches_failed_total[5m])",
            "legendFormat": "Failed batches/sec"
          },
          {
            "expr": "rate(mdp_sink_dropped_batches_total[5m])",
            "legendFormat": "Dropped batches/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Source Status",
        "type": "table",
        "targets": [
          {
            "expr": "mdp_source_status",
            "legendFormat": "{{source}} - {{tenant}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
