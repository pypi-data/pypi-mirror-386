msgid "Correct the errors first, please."
msgstr "Prosím, nejdříve opravte chyby."

msgid "File size"
msgstr "Velikost souboru"

#, python-format
msgid "Max. %s file with a total size of max. %s"
msgid_plural "Max. %s files with a total size of max. %s"
msgstr[0] "Max. %s soubor o celkové velikosti max. %s"
msgstr[1] "Max. %s soubory o celkové velikosti max. %s"
msgstr[2] "Max. %s souborů o celkové velikosti max. %s"

msgid "Max. size"
msgstr "Max. velikost"

msgid "Please wait. Submitting form..."
msgstr "Prosím čekejte. Formulář se odesílá..."

msgid "Remove file."
msgstr "Odebrat soubor."

msgid "The file size has exceeded the specified limit."
msgstr "Velikost souboru přesáhla stanovený limit."

msgid "The file type is not among the accpeted types."
msgstr "Typ souboru není mezi povolenými typy."

msgid "The total size of all files has exceeded the specified limit."
msgstr "Celková velikost všech souborů přesáhla stanovený limit."

msgid "This file exceeds the uploaded files limit."
msgstr "Tento soubor překračuje limit souborů pro nahrání."

#, python-format
msgid "You have to choose at least %(value)s option (chosen %(chosen)s)."
msgid_plural "You have to choose at least %(value)s options (chosen %(chosen)s)."
msgstr[0] "Musíte vybrat nejméně %(value)s položku (vybráno %(chosen)s)."
msgstr[1] "Musíte vybrat nejméně %(value)s položky (vybráno %(chosen)s)."
msgstr[2] "Musíte vybrat nejméně %(value)s položek (vybráno %(chosen)s)."
