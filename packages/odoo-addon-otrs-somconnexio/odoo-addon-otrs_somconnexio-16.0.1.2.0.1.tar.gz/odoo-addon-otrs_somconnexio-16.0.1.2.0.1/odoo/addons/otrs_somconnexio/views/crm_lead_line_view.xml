    <field name="model">crm.lead.line</field>
    <field name="inherit_id" ref="somconnexio.view_form_lead_line_mobile"/>
    <field name="arch" type="xml">
      <field name="iban" position="after">
        <field name="ticket_number"/>
      </field>
    </field>
  </record>
  <record id="view_form_lead_line_broadband" model="ir.ui.view">
    <field name="name">crm.lead.line broadband form view</field>
    <field name="model">crm.lead.line</field>
    <field name="inherit_id" ref="somconnexio.view_form_lead_line_broadband"/>
