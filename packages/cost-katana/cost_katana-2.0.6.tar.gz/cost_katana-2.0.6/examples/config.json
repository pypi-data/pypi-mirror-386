{
  "api_key": "dak_your_api_key_here",
  "base_url": "https://cost-katana-backend.store",
  "default_model": "nova-lite",
  "default_temperature": 0.7,
  "default_max_tokens": 2000,
  "cost_limit_per_request": 1.0,
  "cost_limit_per_day": 50.0,
  "enable_analytics": true,
  "enable_optimization": true,
  "enable_failover": true,
  "model_mappings": {
    "gemini": "gemini-2.0-flash-exp",
    "claude": "anthropic.claude-3-sonnet-20240229-v1:0",
    "gpt4": "gpt-4-turbo-preview",
    "nova": "amazon.nova-pro-v1:0"
  },
  "providers": {
    "google": {
      "priority": 1,
      "models": ["gemini-2.0-flash", "gemini-pro", "gemini-flash"],
      "preferred_for": ["creative_writing", "general_chat", "multimodal"]
    },
    "anthropic": {
      "priority": 2,
      "models": ["claude-3-sonnet", "claude-3-haiku", "claude-3.5-sonnet"],
      "preferred_for": ["analysis", "coding", "reasoning", "safety"]
    },
    "openai": {
      "priority": 3,
      "models": ["gpt-4", "gpt-4-turbo", "gpt-3.5-turbo"],
      "preferred_for": ["function_calling", "structured_output", "embeddings"]
    },
    "aws": {
      "priority": 4,
      "models": ["nova-pro", "nova-lite", "nova-micro"],
      "preferred_for": ["enterprise", "batch_processing", "cost_optimization"]
    }
  },
  "cost_optimization": {
    "enable_caching": true,
    "cache_ttl": 3600,
    "enable_compression": true,
    "auto_fallback": true,
    "smart_routing": true,
    "batch_requests": false
  },
  "monitoring": {
    "track_usage": true,
    "alert_on_high_cost": true,
    "daily_spend_alert": 25.0,
    "monthly_spend_alert": 500.0
  }
}