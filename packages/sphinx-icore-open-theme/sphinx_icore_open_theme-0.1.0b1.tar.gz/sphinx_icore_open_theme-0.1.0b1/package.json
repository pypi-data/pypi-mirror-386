{
  "name": "@minvws/icore-manon-sphinx-theme",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run copy:fonts && npm run copy:js && npm run build:css",
    "build:css": "sass static/scss/index.scss --style=compressed --no-source-map src/sphinx_icore_open/theme/sphinx_icore_open/static/css/theme.css --load-path=node_modules/",
    "watch:css": "sass --watch static/scss/index.scss:src/sphinx_icore_open/theme/sphinx_icore_open/static/css/theme.css --style=compressed --no-source-map --load-path=node_modules/",
    "copy:fonts": "cp -r node_modules/@minvws/manon-themes/icore-open/fonts/* src/sphinx_icore_open/theme/sphinx_icore_open/static/fonts/",
    "copy:js": "cp node_modules/@minvws/manon/*.js src/sphinx_icore_open/theme/sphinx_icore_open/static/js/"
  },
  "dependencies": {
    "@minvws/manon": "^17.0.0",
    "@minvws/manon-themes": "^17.0.0"
  },
  "devDependencies": {
    "sass": "^1.92.1"
  }
}
