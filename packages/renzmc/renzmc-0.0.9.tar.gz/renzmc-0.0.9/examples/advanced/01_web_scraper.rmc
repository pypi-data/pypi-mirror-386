// Statistik
tampilkan "\n=== STATISTIK ==="
cities itu []

untuk setiap item dari data
    city itu item["city"]
    
    // Cek apakah city sudah ada di list
    city_ada itu salah
    untuk setiap c dari cities
        jika c == city
            city_ada itu benar
        selesai
    selesai
    
    // Tambah jika belum ada
    jika tidak city_ada
        tambah(cities, city)
    selesai
selesai

