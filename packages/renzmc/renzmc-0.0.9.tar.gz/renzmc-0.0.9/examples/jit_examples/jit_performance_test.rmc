mulai_waktu1 itu waktu()

untuk iterasi dari 1 sampai 20
    x itu iterasi * 2
    y itu iterasi * 3  
    z itu iterasi * 4
    
    hasil itu hitung_kompleks(x, y, z)
    
    jika iterasi <= 10
        tampilkan f"Iterasi {iterasi} (sebelum JIT): hasil = {hasil}"
    lainnya
        tampilkan f"Iterasi {iterasi} (setelah JIT): hasil = {hasil}"
    selesai
selesai

