# ===================================
# 4. Tuple sebagai Return Value
# ===================================
tampilkan "4. Tuple sebagai Return Value:"

fungsi get_min_max(numbers):
    min_val itu numbers[0]
    max_val itu numbers[0]
    
    untuk setiap num dari numbers
        jika num < min_val
            min_val itu num
        selesai
        jika num > max_val
            max_val itu num
        selesai
    selesai
    
    hasil (| min_val, max_val |)
selesai

angka itu [5, 2, 9, 1, 7, 3]
tampilkan "Angka: " + ke_teks(angka)

min_max itu get_min_max(angka)
tampilkan "Min-Max tuple: " + ke_teks(min_max)
tampilkan "Min: " + ke_teks(min_max[0])
tampilkan "Max: " + ke_teks(min_max[1])

tampilkan ""

