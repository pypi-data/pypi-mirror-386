# ===================================
# 9. Tuple Praktis: Koordinat
# ===================================
tampilkan "9. Tuple Praktis: Koordinat:"

# Fungsi untuk menghitung jarak
fungsi hitung_jarak(p1, p2):
    dx itu p2[0] - p1[0]
    dy itu p2[1] - p1[1]
    jarak itu ((dx * dx) + (dy * dy)) ** 0.5
    hasil jarak
selesai

point1 itu (| 0, 0 |)
point2 itu (| 3, 4 |)

tampilkan "Point 1: " + ke_teks(point1)
tampilkan "Point 2: " + ke_teks(point2)

jarak itu hitung_jarak(point1, point2)
tampilkan "Jarak: " + ke_teks(jarak)

tampilkan ""

