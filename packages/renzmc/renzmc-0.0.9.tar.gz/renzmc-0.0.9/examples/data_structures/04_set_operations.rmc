# ===================================
# 1. Membuat Set
# ===================================
tampilkan "1. Membuat Set:"

# Set dengan angka
set1 itu {| 1, 2, 3, 4, 5 |}
tampilkan "Set angka: " + ke_teks(set1)

# Set dengan string
set2 itu {| "apel", "jeruk", "mangga" |}
tampilkan "Set string: " + ke_teks(set2)

# Set dengan duplikat (otomatis dihapus)
set3 itu {| 1, 2, 2, 3, 3, 3, 4 |}
tampilkan "Set dengan duplikat: " + ke_teks(set3)
tampilkan "Duplikat otomatis dihapus!"

# Set kosong
set_kosong itu {| |}
tampilkan "Set kosong: " + ke_teks(set_kosong)

tampilkan ""

