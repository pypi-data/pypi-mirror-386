# ===================================
# 7. Set Praktis: Remove Duplicates
# ===================================
tampilkan "7. Set Praktis: Remove Duplicates:"

# Fungsi untuk remove duplicates
fungsi remove_duplicates(lst):
    # Buat set baru
    result_set itu {| |}
    result_list itu []
    
    untuk setiap item dari lst
        # Check if not in result
        jika tidak (item dalam result_list)
            result_list itu result_list + [item]
        selesai
    selesai
    
    hasil result_list
selesai

data_duplikat itu [1, 2, 3, 2, 4, 3, 5, 1, 6]
tampilkan "Data dengan duplikat: " + ke_teks(data_duplikat)
tampilkan "Panjang: " + ke_teks(panjang(data_duplikat))

data_unik itu remove_duplicates(data_duplikat)
tampilkan "Data unik: " + ke_teks(data_unik)
tampilkan "Panjang: " + ke_teks(panjang(data_unik))

tampilkan ""

# ===================================
# 8. Set Praktis: Unique Counter
# ===================================
tampilkan "8. Set Praktis: Unique Counter:"

# Count unique elements
words itu ["hello", "world", "hello", "python", "world"]
tampilkan "Words: " + ke_teks(words)

# Manual unique count
unique_words itu []
untuk setiap word dari words
    jika tidak (word dalam unique_words)
        unique_words itu unique_words + [word]
    selesai
selesai

tampilkan "Unique words: " + ke_teks(unique_words)
tampilkan "Total words: " + ke_teks(panjang(words))
tampilkan "Unique count: " + ke_teks(panjang(unique_words))

tampilkan ""

