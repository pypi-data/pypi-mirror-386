{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "lt reset",
      "type": "debugpy",
      "request": "launch",
      "module": "log_time_to_tempo.cli.main",
      "args": ["-vv", "reset", "-f"]
    },
    {
      "name": "lt init",
      "type": "debugpy",
      "request": "launch",
      "module": "log_time_to_tempo.cli.main",
      "args": ["-vv", "init"]
    },
    {
      "name": "lt log",
      "type": "debugpy",
      "request": "launch",
      "module": "log_time_to_tempo.cli.main",
      // "args": ["-vv", "log", "2h"]
      "args": ["-vv", "log", "2h", "--from-time", "13"]
      // "args": ["-vv", "log", "--day", "28.2"]
    },
    {
      "name": "lt list",
      "type": "debugpy",
      "request": "launch",
      "module": "log_time_to_tempo.cli.main",
      // "args": ["-vv", "log", "2h"]
      "args": ["-vv", "list", "week"]
    },
    {
      "name": "lt (custom command)",
      "type": "debugpy",
      "request": "launch",
      "module": "log_time_to_tempo.cli.main",
      // "args": ["-vv", "log", "2h"]
      "args": "${command:pickArgs}"
      // "args": ["-vv", "log", "--day", "28.2"]
    }
  ]
}
