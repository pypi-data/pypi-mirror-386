#: src/pyams_auth_apikey/plugin.py:159
#: src/pyams_auth_apikey/zmi/__init__.py:218
#, python-format
msgid "API key: {}"
msgstr "Clé d'API : {}"

#: src/pyams_auth_apikey/plugin.py:367
msgid "API keys authentication"
msgstr "Authentification par clés d'API"

#: src/pyams_auth_apikey/interfaces.py:34
msgid "API keys authentication plug-in"
msgstr "Module d'authentification par clés d'API"

#: src/pyams_auth_apikey/interfaces.py:47
msgid "Key name"
msgstr "Nom de la clé"

#: src/pyams_auth_apikey/interfaces.py:48
msgid ""
"This name must be unique between all API keys; if empty, a random name will "
"be used as name"
msgstr ""
"Le nom de la clé doit être unique, et il est utilisé notamment pour "
"identifier cette clé en tant que mandataire ; s'il n'est pas indiqué, un nom "
"aléatoire lui sera attribué"

#: src/pyams_auth_apikey/interfaces.py:52
msgid "API key"
msgstr "Clé d'API"

#: src/pyams_auth_apikey/interfaces.py:53
msgid ""
"This API key will never be displayed again, just copy it and keep it safe!"
msgstr ""
"La clé d'API ne sera jamais visible à nouveau, vous devez la copier et la "
"conserver en sécurité !"

#: src/pyams_auth_apikey/interfaces.py:56
msgid "API key hash"
msgstr "Empreinte"

#: src/pyams_auth_apikey/interfaces.py:57
msgid "This is a hash of the API key and can't be modified"
msgstr "Cette clé de hashage identifie la clé et ne peut pas être modifiée"

#: src/pyams_auth_apikey/interfaces.py:61
msgid "Enabled key?"
msgstr "Clé active ?"

#: src/pyams_auth_apikey/interfaces.py:62
msgid "Select 'no' to disable the key temporarily"
msgstr "Sélectionnez 'non' pour désactiver cette clé temporairement"

#: src/pyams_auth_apikey/interfaces.py:66
msgid "Key label"
msgstr "Libellé"

#: src/pyams_auth_apikey/interfaces.py:67
msgid "This label will be used to identify the key"
msgstr "Ce libellé pourra être utilisé pour identifier la clé"

#: src/pyams_auth_apikey/interfaces.py:70
msgid "Associated principal"
msgstr "Mandataire associé"

#: src/pyams_auth_apikey/interfaces.py:71
msgid ""
"If defined, this will identify the principal which will be used when a "
"request will be authenticated with this API key"
msgstr ""
"S'il est défini, ce mandataire sera utilisé en lieu et place de la clé pour "
"authentifier les requêtes qui l'utiliseront"

#: src/pyams_auth_apikey/interfaces.py:79
msgid "Selected principal can't be another API key!"
msgstr "Le mandataire sélectionné ne peut pas être une autre clé d'API !"

#: src/pyams_auth_apikey/interfaces.py:84
#: src/pyams_auth_apikey/zmi/container.py:159
msgid "Activation date"
msgstr "Date d'activation"

#: src/pyams_auth_apikey/interfaces.py:85
msgid "This key will be enabled only after this date"
msgstr "La clé ne sera active qu'à partir de cette date"

#: src/pyams_auth_apikey/interfaces.py:88
#: src/pyams_auth_apikey/zmi/container.py:176
msgid "Expiration date"
msgstr "Date d'expiration"

#: src/pyams_auth_apikey/interfaces.py:89
msgid "This key will not be enabled after this date"
msgstr "La clé ne sera pas active au-delà de cette date"

#: src/pyams_auth_apikey/interfaces.py:92
msgid "Restrict referrers"
msgstr "Restreindre les référents"

#: src/pyams_auth_apikey/interfaces.py:93
msgid "If this option is enabled, only selected referrers will be enabled"
msgstr ""
"Si cette option est activée, seuls les référents indiqués ci-dessous seront "
"autorisés"

#: src/pyams_auth_apikey/interfaces.py:97
msgid "Allowed referrers"
msgstr "Référents autorisés"

#: src/pyams_auth_apikey/interfaces.py:98
msgid "Only selected referrers will be allowed to use this API key"
msgstr ""
"Seules les requêtes dont l'origine correspond à l'un de ces référents seront "
"authentifiées"

#: src/pyams_auth_apikey/interfaces.py:101
msgid "Allowed as request param"
msgstr "Autorisée comme paramètre de requête"

#: src/pyams_auth_apikey/interfaces.py:102
msgid ""
"If 'yes', this API key can be sent as a request URL parameter instead of an "
"HTTP request header only"
msgstr ""
"Si 'oui', cette clé d'API pourra être transmise via un paramètre de requête et "
"non seulement via un en-tête HTTP"

#: src/pyams_auth_apikey/interfaces.py:107
msgid "Request param name"
msgstr "Nom du paramètre"

#: src/pyams_auth_apikey/interfaces.py:108
msgid "Name of the request parameter used to send this API key"
msgstr "Nom du paramètre de la requête utilisé pour fournir la clé d'API"

#: src/pyams_auth_apikey/interfaces.py:120
msgid "Enabled API key authentication?"
msgstr "Activer l'authentification par clés d'API"

#: src/pyams_auth_apikey/interfaces.py:121
msgid "Enable login via API keys"
msgstr "Activer l'authentification par clés d'API"

#: src/pyams_auth_apikey/interfaces.py:125
msgid "HTTP header"
msgstr "En-tête HTTP"

#: src/pyams_auth_apikey/interfaces.py:126
msgid "Name of the HTTP header used to provide the API key"
msgstr "Nom de l'en-tête HTTP utilisé pour fournir la clé d'API"

#: src/pyams_auth_apikey/zmi/plugin.py:50
#: src/pyams_auth_apikey/zmi/plugin.py:60
msgid "API keys configuration"
msgstr "Configuration des clés d'API"

#: src/pyams_auth_apikey/zmi/container.py:52
#: src/pyams_auth_apikey/zmi/container.py:208
msgid "API keys"
msgstr "Liste des clés d'API"

#: src/pyams_auth_apikey/zmi/container.py:90
msgid "Click icon to enable or disable this API key"
msgstr "Cliquer pour activer ou désactiver cette clé"

#: src/pyams_auth_apikey/zmi/container.py:115
msgid "Name"
msgstr "Nom"

#: src/pyams_auth_apikey/zmi/container.py:127
msgid "Label"
msgstr "Libellé"

#: src/pyams_auth_apikey/zmi/container.py:143
msgid "Principal"
msgstr "Mandataire"

#: src/pyams_auth_apikey/zmi/__init__.py:69
msgid "Add API key"
msgstr "Ajouter une clé d'API"

#: src/pyams_auth_apikey/zmi/__init__.py:83
msgid "New API key"
msgstr "Nouvelle clé d'API"

#: src/pyams_auth_apikey/zmi/__init__.py:84
msgid "New API key properties"
msgstr "Propriété de la nouvelle clé"

#: src/pyams_auth_apikey/zmi/__init__.py:131
#: src/pyams_auth_apikey/zmi/__init__.py:241
msgid "Plug-in: API keys authentication"
msgstr "Module : Authentification par clés d'API"

#: src/pyams_auth_apikey/zmi/__init__.py:160
msgid ""
"WARNING: the API key won't be displayed after it's creation!\n"
"You must copy it NOW and keep it safe!!"
msgstr ""
"ATTENTION : cette clé ne sera plus affichée après sa création !\n"
"Vous devez la copier MAINTENANT et la conserver en lieu sûr !!"

#: src/pyams_auth_apikey/zmi/__init__.py:174
msgid "This key name is already used!"
msgstr "Ce nom de clé est déjà utilisé !"

#: src/pyams_auth_apikey/zmi/__init__.py:220
msgid "API key properties"
msgstr "Propriété de la clé"

#~ msgid "Plug-in: OAuth authentication"
#~ msgstr "Module : Authentification par clés d'API"
