{
  "mappings": {
    "dynamic_templates": [
      {
        "creators": {
          "path_match": "created_by.*",
          "mapping": {
            "type": "keyword"
          }
        }
      }
    ],
    "dynamic": "strict",
    "properties": {
      "$schema": {
        "type": "keyword"
      },
      "created": {
        "type": "date"
      },
      "updated": {
        "type": "date"
      },
      "uuid": {
        "type": "keyword"
      },
      "version_id": {
        "type": "keyword"
      },

      "created_by": {
        "type": "object",
        "dynamic": "true"
      },
      "id": {
        "type": "keyword"
      },
      "type": {
        "type": "keyword"
      },
      "request": {
        "type": "keyword"
      },
      "request_id": {
        "type": "keyword"
      },
      "payload": {
        "type": "object",
        "enabled": false
      }
    }
  }
}
