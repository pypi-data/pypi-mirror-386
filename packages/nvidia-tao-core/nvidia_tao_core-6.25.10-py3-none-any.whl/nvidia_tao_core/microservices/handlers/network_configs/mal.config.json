{
    "api_params": {
        "dataset_type": "segmentation",
        "actions": [
            "train",
            "evaluate",
            "inference"
        ],
        "formats": [
            "default"
        ],
        "accepted_ds_intents": [
            "training",
            "evaluation"
        ],
        "image": "TAO_PYTORCH",
        "spec_backend": "yaml",
        "actions_pipe": {
            "train": "train",
            "evaluate": "evaluate",
            "inference": "inference"
        }
    },
    "data_sources": {
        "train": {
            "dataset.train_img_dir": {
                "source": "train_datasets",
                "multiple_sources": false,
                "path": "images.tar.gz"
            },
            "dataset.train_ann_path": {
                "source": "train_datasets",
                "multiple_sources": false,
                "path": "annotations.json"
            },
            "dataset.val_img_dir": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "images.tar.gz"
            },
            "dataset.val_ann_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "annotations.json"
            }
        },
        "evaluate": {
            "dataset.val_img_dir": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "images.tar.gz"
            },
            "dataset.val_ann_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "annotations.json"
            }
        },
        "inference": {
            "inference.img_dir": {
                "source": "inference_dataset",
                "multiple_sources": false,
                "path": "images.tar.gz"
            },
            "inference.ann_path": {
                "source": "inference_dataset",
                "multiple_sources": false,
                "path": "annotations.json"
            }
        }
    },
    "spec_params": {
        "train": {
            "results_dir": "output_dir"
        },
        "evaluate": {
            "results_dir": "output_dir",
            "evaluate.checkpoint": "parent_model"
        },
        "inference": {
            "results_dir": "output_dir",
            "inference.label_dump_path": "create_inference_result_file_mal",
            "inference.checkpoint": "parent_model"
        }
    },
    "metrics": {
        "available_metrics": [
            "mIoU"
        ],
        "monitoring_metric": "mIoU"
    },
    "cloud_upload": {
        "upload_strategy": {
            "inference": "tarball_after_completion"
        }
    }
}