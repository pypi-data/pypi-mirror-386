{
    "api_params": {
        "dataset_type": "ocdnet",
        "actions": [
            "train",
            "evaluate",
            "export",
            "prune",
            "retrain",
            "inference",
            "gen_trt_engine"
        ],
        "formats": [
            "default"
        ],
        "accepted_ds_intents": [
            "training",
            "evaluation"
        ],
        "image": "TAO_PYTORCH",
        "spec_backend": "yaml",
        "actions_pipe": {
            "train": "train",
            "evaluate": "evaluate",
            "prune": "prune_with_spec",
            "retrain": "retrain",
            "export": "export_with_spec",
            "inference": "inference",
            "gen_trt_engine": "gen_trt_engine"
        }
    },
    "dataset_validation": {
        "required_files": {
            "default": [
                {
                    "any_of": [
                        {
                            "path": "train.tar.gz",
                            "type": "file"
                        },
                        {
                            "all_of": [
                                {
                                    "path": "test.tar.gz",
                                    "type": "file"
                                },
                                {
                                    "path": "test/img.tar.gz",
                                    "type": "file"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    },
    "dynamic_config": {
        "parent_action_rules": {
            "retrain": {
                "set_value": {
                    "model.load_pruned_graph": true
                }
            }
        },
        "action_rules": {
            "retrain": {
                "set_value": {
                    "model.load_pruned_graph": true
                }
            }
        }
    },
    "data_sources": {
        "train": {
            "dataset.train_dataset.data_path": {
                "source": "train_datasets",
                "multiple_sources": false,
                "path": "train.tar.gz",
                "transform": "wrap_in_list"
            },
            "dataset.validate_dataset.data_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "test.tar.gz",
                "transform": "wrap_in_list"
            }
        },
        "evaluate": {
            "dataset.validate_dataset.data_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "test.tar.gz",
                "transform": "wrap_in_list"
            }
        },
        "prune": {
            "dataset.validate_dataset.data_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "test.tar.gz",
                "transform": "wrap_in_list"
            }
        },
        "retrain": {
            "dataset.train_dataset.data_path": {
                "source": "train_datasets",
                "multiple_sources": false,
                "path": "train.tar.gz",
                "transform": "wrap_in_list"
            },
            "dataset.validate_dataset.data_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "test.tar.gz",
                "transform": "wrap_in_list"
            }
        },
        "inference": {
            "inference.input_folder": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "test/img.tar.gz"
            }
        },
        "gen_trt_engine": {
            "gen_trt_engine.tensorrt.calibration.cal_image_dir": {
                "source": "calibration_dataset",
                "multiple_sources": false,
                "path": "train/img.tar.gz",
                "transform": "wrap_in_list"
            }
        }
    },
    "spec_params": {
        "train": {
            "results_dir": "output_dir",
            "model.pretrained_model_path": "ptm_if_no_resume_model",
            "train.resume_training_checkpoint_path": "resume_model"
        },
        "evaluate": {
            "results_dir": "output_dir",
            "evaluate.checkpoint": "parent_model",
            "model.pruned_graph_path": "pruned_model",
            "evaluate.trt_engine": "parent_model"
        },
        "inference": {
            "results_dir": "output_dir",
            "inference.checkpoint": "parent_model",
            "model.pruned_graph_path": "pruned_model",
            "inference.trt_engine": "parent_model"
        },
        "prune": {
            "results_dir": "output_dir",
            "prune.checkpoint": "parent_model"
        },
        "retrain": {
            "results_dir": "output_dir",
            "model.pruned_graph_path": "parent_model"
        },
        "export": {
            "results_dir": "output_dir",
            "export.checkpoint": "parent_model",
            "export.onnx_file": "create_onnx_file"
        },
        "gen_trt_engine": {
            "results_dir": "output_dir",
            "gen_trt_engine.onnx_file": "parent_model",
            "gen_trt_engine.trt_engine": "create_engine_file",
            "gen_trt_engine.tensorrt.calibration.cal_cache_file": "create_cal_cache"
        }
    },
    "automl_spec_params": {
        "results_dir": "automl_output_dir",
        "model.pretrained_model_path": "automl_assign_ptm",
        "train.resume_training_checkpoint_path": "automl_resume_model",
        "train.checkpoint_interval": "assign_const_value,train.num_epochs",
        "train.validation_interval": "assign_const_value,train.num_epochs,train.checkpoint_interval"
    },
    "metrics": {
        "available_metrics": [
            "hmean"
        ],
        "monitoring_metric": "hmean"
    },
    "cloud_upload": {
        "upload_strategy": {
            "inference": "tarball_after_completion"
        }
    }
}