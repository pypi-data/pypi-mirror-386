{
    "api_params": {
        "dataset_type":"maxine_dataset",
        "actions": ["train", "export", "gen_trt_engine"],
        "formats":["raw", "maxine_gaze"],
        "accepted_ds_intents": ["training", "evaluation"],
        "image":"MAXINE_DLDK",
        "spec_backend":"yaml",
        "actions_pipe":{
            "train":"train",
            "export":"export_with_spec",
            "gen_trt_engine":"gen_trt_engine"
        }
    },

    "data_sources": {
        "train": {
            "train_dataset_path": {
                "source": "train_datasets",
                "multiple_sources": false,
                "path": "data.h5"
            },
            "val_dataset_path": {
                "source": "eval_dataset",
                "multiple_sources": false,
                "path": "data.h5"
            }
        }
    },

    "spec_params":{
        "train":{
            "checkpoint_path": "ptm_if_no_resume_model",
            "results_dir":"output_dir"
        },
        "export":{
            "checkpoint_path": "parent_model",
            "results_dir":"output_dir"
        },
        "gen_trt_engine":{
            "input_dir":"parent_model_folder",
            "results_dir":"output_dir"
        }
    }
}