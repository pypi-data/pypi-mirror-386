x-samos-type-name: {{data['x-samos-type-name']}}
x-samos-namespace: {{data['x-samos-namespace']}}

title: {{data.title}}
description: {{data.description}}

x-samos-extends-types:
  - type-name: Machine
    subtype-priority: 3

x-samos-keys:
  {% for k in data['x-samos-keys'] %}
  - {{k}}
  {% endfor %}

x-samos-table:
  {% for k in data['x-samos-table'] %}
  - name: {{k}}
  {% endfor %}

x-samos-detail:
  {% for k in data['x-samos-detail'] %}
  - name: {{k}}
  {% endfor %}

properties:
  {{ data.properties | to_yaml | indent(2) }}