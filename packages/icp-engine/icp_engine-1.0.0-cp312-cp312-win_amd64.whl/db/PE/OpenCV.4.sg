// https://github.com/opencv/opencv
// https://github.com/shimat/opencvsharp
init("library", "OpenCV");

function detect() {
    if (PE.isNET() && PE.isNetObjectPresent("OpenCvSharp")) {
        bDetected = true;
    } else if (PE.isLibraryPresentExp(/^opencv/i)) {
        bDetected = true;
    }

    return result();
}