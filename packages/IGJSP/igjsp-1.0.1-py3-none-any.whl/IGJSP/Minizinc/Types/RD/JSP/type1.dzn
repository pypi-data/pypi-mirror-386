%SETS
JOBS     = 1..{jobs};
MACHINES = 1..{machines};
SPEED    = {Speed};

time               = array3d(JOBS,MACHINES,1..SPEED,{time});
energy             = array3d(JOBS,MACHINES,1..SPEED,{energy});
precedence         = array2d(JOBS,MACHINES,{precedence});
releaseDate        = {releaseDate};
dueDate            = {dueDate};