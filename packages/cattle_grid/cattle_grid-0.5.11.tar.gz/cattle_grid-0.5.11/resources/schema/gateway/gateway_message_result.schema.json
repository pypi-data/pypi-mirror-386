{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "title": "GatewayMessageResult",
  "description": "Message send from the gateway",
  "properties": {
    "action": {
      "type": "string",
      "enum": ["self", "fetch_result", "incoming", "profile", "options"],
      "description": "Action taken by the gateway"
    },
    "actor": {
      "type": "string",
      "description": "actor_id of the actor that is receiving the message."
    },
    "data": {
      "type": "object",
      "description": "Received data."
    }
  },
  "required": ["action", "data", "actor"]
}
