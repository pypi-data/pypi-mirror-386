<?xml version='1.0' encoding='utf-8'?>
<interface version="1.5" author="PygubuDesigner 0.41.4">
  <project>
    <settings>
      <setting id="name">Counter</setting>
      <setting id="description">Examples based on site [7GUIs](https://eugenkiss.github.io/7guis/tasks)</setting>
      <setting id="module_name">counterapp</setting>
      <setting id="template">application</setting>
      <setting id="main_widget">toplevel1</setting>
      <setting id="main_classname">CounterApp</setting>
      <setting id="main_menu" />
      <setting id="output_dir" />
      <setting id="output_dir2" />
      <setting id="import_tkvariables">True</setting>
      <setting id="use_ttk_styledefinition_file">False</setting>
      <setting id="use_i18n">False</setting>
      <setting id="all_ids_attributes">False</setting>
      <setting id="generate_code_onsave">False</setting>
      <setting id="use_window_centering_code">False</setting>
      <setting id="ttk_style_definition_file" />
    </settings>
    <customwidgets />
    <resources />
  </project>
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="padx">5</property>
    <property name="pady">5</property>
    <property name="title" translatable="yes">Counter</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Entry" id="entry1">
        <property name="font">{Helvetica} 12 {}</property>
        <property name="state">readonly</property>
        <property name="textvariable">int:counter_var</property>
        <property name="width">10</property>
        <layout manager="pack">
          <property name="fill">both</property>
          <property name="padx">0 20</property>
          <property name="side">left</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="button1">
        <property name="command" type="command" cbtype="simple">on_count_clicked</property>
        <property name="text" translatable="yes">Count</property>
        <layout manager="pack">
          <property name="fill">both</property>
          <property name="side">left</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
