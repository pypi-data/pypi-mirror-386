trait RearLeftWheelSuspension:
    rear_left_suspension becomes Vehicles.Wheels.Interactions.Suspension:
        wheel: rear_left_wheel
        suspension_connector: chassis.rear_left_connector
    chassis.rear_left_connector is Vehicles.Wheels.Charges.SuspensionConnector:
        forward_vector: chassis.forward_vector
        up_vector: chassis.up_vector
