trait FrontLeftWheelSuspension:
    front_left_suspension becomes Vehicles.Wheels.Interactions.Suspension with Vehicles.Traits.Steering:
        wheel: front_left_wheel
        suspension_connector: chassis.front_left_connector
    chassis.front_left_connector is Vehicles.Wheels.Charges.SuspensionConnector:
        forward_vector: chassis.forward_vector
        up_vector: chassis.up_vector
