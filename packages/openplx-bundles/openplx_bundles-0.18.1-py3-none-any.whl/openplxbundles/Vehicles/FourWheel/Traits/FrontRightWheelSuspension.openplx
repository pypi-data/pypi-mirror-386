trait FrontRightWheelSuspension:
    front_right_suspension becomes Vehicles.Wheels.Interactions.Suspension with Vehicles.Traits.Steering:
        wheel: front_right_wheel
        suspension_connector: chassis.front_right_connector
    chassis.front_right_connector is Vehicles.Wheels.Charges.SuspensionConnector:
        forward_vector: chassis.forward_vector
        up_vector: chassis.up_vector
