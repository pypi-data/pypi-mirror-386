prune bin
include bin/sage-download-file
include bin/sage-get-system-packages
include bin/sage-guess-package-system
include bin/sage-package
include bin/sage-print-system-package-command
include bin/sage-spkg-info
include bin/sage-uncompress-spkg
graft sage_root/.upstream.d
graft sage_root/build/pkgs
prune sage_root/build/pkgs/*/src*
# Exclude files generated by cibuildwheel targets
prune sage_root/build/pkgs/*/unpacked
#
global-exclude .tox
global-exclude *~*
global-exclude *.bak
global-exclude *.orig
global-exclude __pycache__
global-exclude *.py[co]
global-exclude *.so
global-exclude .DS_Store
