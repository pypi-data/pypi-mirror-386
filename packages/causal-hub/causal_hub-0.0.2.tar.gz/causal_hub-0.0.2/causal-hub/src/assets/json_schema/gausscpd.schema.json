                "sample_design_covariance": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                },
                "sample_size": {
                    "type": "number"
                }
            }
        },
        "sample_log_likelihood": {
            "type": [
                "number",
                "null"
            ]
        },
        "type": {
            "type": "string",
            "const": "gausscpd"
