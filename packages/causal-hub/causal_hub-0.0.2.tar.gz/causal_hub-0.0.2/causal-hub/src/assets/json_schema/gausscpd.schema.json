                        "items": {
                            "type": "number"
                        }
                    }
                },
                "intercept": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                },
                "covariance": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "sample_statistics": {
            "type": [
                "object",
