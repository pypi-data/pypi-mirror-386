{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "catcpd.schema.json",
    "type": "object",
    "properties": {
        "states": {
            "type": "object",
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "conditioning_states": {
            "type": "object",
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "parameters": {
            "type": "array",
            "items": {
                "type": "array",
                "items": {
                    "type": "number"
                }
            }
        },
        "sample_statistics": {
            "type": [
                "object",
                "null"
            ],
            "properties": {
                "sample_conditional_counts": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                },
                "sample_size": {
                    "type": "number"
                }
            }
        },
        "sample_log_likelihood": {
            "type": [
                "number",
                "null"
            ]
        },
        "type": {
            "type": "string",
            "const": "catcpd"
        }
    },
    "required": [
        "states",
        "conditioning_states",
        "parameters",
        "type"
    ]
}