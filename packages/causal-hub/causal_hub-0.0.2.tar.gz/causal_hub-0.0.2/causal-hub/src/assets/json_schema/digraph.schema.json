{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "digraph.schema.json",
    "type": "object",
    "properties": {
        "labels": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "edges": {
            "type": "array",
            "items": {
                "type": "array",
                "items": {
                    "type": "string"
                },
                "minItems": 2,
                "maxItems": 2
            }
        },
        "type": {
            "type": "string",
            "const": "digraph"
        }
    },
    "required": [
        "labels",
        "edges",
        "type"
    ]
}